                                                </li>
                                            </ul>
                                        </div><!-- Shortcuts -->
                                    </aside>
                                </div>

                                <div class="col-lg-6">
                                    @RenderBody()                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </section>


    </div>
    
    <script src="~/Scripts/main.min.js"></script>
    <script src="~/Scripts/script.js"></script>
    <script src="~/Scripts/map-init.js"></script>
