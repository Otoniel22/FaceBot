<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")  
</head>
<body>
    
    
    <!--<div class="se-pre-con"></div>-->
    <div class="theme-layout">

        <!-- Encabezado -->

        <div class="topbar stick">
            <div class="logo">
                <a href="index.html"><h3>FaceBot</h3></a>
            </div>

            <div class="top-area">
                <ul class="main-menu">
                    <li>
                        <a href="index.html" title="">Inicio</a>
                    </li>
                    <li>
                        <a href="MiPerfil.html" title="">Mi perfil</a>
                    </li>
                    <li>
                        <a href="#" title="">Configuración</a>
