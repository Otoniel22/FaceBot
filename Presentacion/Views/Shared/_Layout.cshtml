                            <li><a href="CambiarContrasenia.html" title="">Cambiar contraseña</a></li>
                            <li><a href="PrivacidadDatos.html" title="">Datos personales</a></li>
                        </ul>
                    </li>

                </ul>

                <!-- buscador -->
                <ul class="setting-area">
                    <li>
                        <a href="" title="Home" data-ripple=""><i class="ti-search"></i></a>
                        <div class="searched">
                            <form method="post" class="form-search">
                                <input type="text" placeholder="Buscar un Amigo">
                                <button data-ripple><i class="ti-search"></i></button>
                            </form>
                        </div>
                    </li>
                    <!-- Notificaciones -->
                    <li>
                        <a href="#" title="Notification" data-ripple="">
